    },
    {
      "cell_type": "code",
      "source": [
        "from imblearn.over_sampling import SMOTE\n"
      ],
      "metadata": {
        "id": "XhUXC_Xr5ttA"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "smote = SMOTE(random_state=42)\n",
        "\n",
        "X_train_smote, y_train_smote = smote.fit_resample(X_train, y_train)\n",
        "\n",
        "print(\"Avant SMOTE:\", y_train.value_counts())\n",
        "print(\"Après SMOTE:\", y_train_smote.value_counts())\n"
      ],
      "metadata": {
        "id": "PqyZ99ur5wbK"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "xgb_smote = XGBClassifier(\n",
        "    n_estimators=200,\n",
        "    max_depth=5,\n",
        "    learning_rate=0.1,\n",
        "    subsample=0.8,\n",
        "    colsample_bytree=0.8,\n",
        "    eval_metric='logloss',\n",
        "    random_state=42\n",
        ")\n"
      ],
      "metadata": {
        "id": "FzBpmg5R5zBC"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "xgb_smote.fit(X_train_smote, y_train_smote)\n"
      ],
      "metadata": {
        "id": "o-hu5bQ151Yo"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "y_pred_xgb_smote = xgb_smote.predict(X_test)\n",
        "\n",
        "y_proba_xgb_smote = xgb_smote.predict_proba(X_test)[:, 1]\n",
        "\n",
        "print(\"Confusion Matrix (XGBoost + SMOTE):\")\n",
        "print(confusion_matrix(y_test, y_pred_xgb_smote))\n",
        "\n",
        "print(\"\\nClassification Report:\")\n",
        "print(classification_report(y_test, y_pred_xgb_smote))\n",
        "\n",
        "print(\"\\nROC-AUC:\", roc_auc_score(y_test, y_proba_xgb_smote))\n"
      ],
      "metadata": {
        "id": "TGksmozT54G4"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.metrics import precision_recall_curve\n",
        "\n",
        "precision, recall, thresholds = precision_recall_curve(y_test, y_proba_xgb_smote)\n"
      ],
      "metadata": {
        "id": "gZtIOI_E58F2"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "plt.figure()\n",
        "plt.plot(thresholds, recall[:-1])\n",
        "plt.xlabel(\"Threshold\")\n",
        "plt.ylabel(\"Recall\")\n",
        "plt.title(\"Recall vs Threshold\")\n",
        "plt.show()\n"
      ],
      "metadata": {
        "id": "Qta2CpBm5809"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "target_recall = 0.90\n",
        "\n",
        "best_threshold = thresholds[recall[:-1] >= target_recall][0]\n",
        "best_threshold\n"
      ],
      "metadata": {
        "id": "j_A9uvn55_Xt"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "y_pred_opt = (y_proba_xgb_smote >= best_threshold).astype(int)\n",
        "\n",
        "print(\"Confusion Matrix (Threshold optimisé):\")\n",
        "print(confusion_matrix(y_test, y_pred_opt))\n",
        "\n",
        "print(\"\\nClassification Report:\")\n",
        "print(classification_report(y_test, y_pred_opt))\n",
        "\n",
        "print(\"\\nROC-AUC:\", roc_auc_score(y_test, y_proba_xgb_smote))\n"
      ],
      "metadata": {
        "id": "70I0QjgV6Bu5"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "f1_scores = 2 * (precision[:-1] * recall[:-1]) / (precision[:-1] + recall[:-1] + 1e-6)\n"
      ],
      "metadata": {
        "id": "wWnKcaWC6D7u"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "valid_idx = recall[:-1] >= 0.90\n",
        "best_idx = np.argmax(f1_scores[valid_idx])\n",
        "\n",
        "best_threshold = thresholds[valid_idx][best_idx]\n",
        "best_threshold\n"
      ],
      "metadata": {
        "id": "sR7UlooU6HxC"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "y_pred_opt = (y_proba_xgb_smote >= best_threshold).astype(int)\n",
        "\n",
        "print(\"Confusion Matrix (Threshold optimisé intelligemment):\")\n",
        "print(confusion_matrix(y_test, y_pred_opt))\n",
        "\n",
        "print(\"\\nClassification Report:\")\n",
        "print(classification_report(y_test, y_pred_opt))\n",
        "\n",
        "print(\"\\nROC-AUC:\", roc_auc_score(y_test, y_proba_xgb_smote))\n"
      ],
      "metadata": {
        "id": "bGDawGr-6KMQ"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.metrics import roc_curve\n",
        "\n",
        "fpr, tpr, _ = roc_curve(y_test, y_proba_xgb_smote)\n",
        "\n",
        "plt.figure()\n",
        "plt.plot(fpr, tpr)\n",
        "plt.xlabel(\"False Positive Rate\")\n",
        "plt.ylabel(\"True Positive Rate\")\n",
        "plt.title(\"ROC Curve - XGBoost + SMOTE\")\n",
        "plt.show()\n"
      ],
      "metadata": {
